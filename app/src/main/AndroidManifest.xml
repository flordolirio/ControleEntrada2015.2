<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.novoexample.controleEntrada">

<uses-permission android:name="android.permission.BLUETOOTH" />
<uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />


<application android:allowBackup="true" android:icon="@mipmap/ic_launcher" android:label="@string/app_name"
    android:theme="@style/AppTheme">
    <activity
        android:name="com.novoexample.controleEntrada.MainActivity"
        android:label="@string/app_name">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

    <activity
        android:name="com.novoexample.controleEntrada.OppenControl"
        android:label="@string/title_activity_led_control"
        android:theme="@style/AppTheme.NoActionBar"></activity>

    <activity
        android:name="com.novoexample.controleEntrada.PairedDevices"
        android:label="Paired Devices"
        android:parentActivityName="com.novoexample.controleEntrada.MainActivity" >
    </activity>

    <activity
        android:name="com.novoexample.controleEntrada.DiscoveredDevices"
        android:label="Discovered Devices"
        android:parentActivityName="com.novoexample.controleEntrada.MainActivity" >
    </activity>
</application>

</manifest>

